{
  "expo": {
    "name": "Markaz Al-Najaax",
    "slug": "mkz-Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/CharColBG.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E1F"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E1E1F"
      },
      "package": "com.hygalac.mkzNative"
    },
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hygalac.mkzNative"
    },
    "plugins": [
      "expo-font",
      "expo-firebase-core"
    ],
    "assetBundlePatterns": [
      "assets/*"
    ],
    "extra": {
      "eas": {
        "projectId": "1593be8a-3265-4818-bfb0-6845afe4dd00"
      }
    }
  }
}
